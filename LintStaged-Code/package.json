{
	"name": "LintStaged-Code",
	"displayName": "LintStaged-Code",
	"description": "LintStaged-Code",
	"version": "0.0.1",
	"publisher": "LintStaged-Code",
	"repository": "https://github.com/jiaozitang/vscodePlugin",
	"engines": {
		"vscode": "^1.34.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:extension.formatProject"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "extension.formatProject",
				"title": "Format Project"
			}
		],
		"menus": {
			"editor/context": [
				{
					"command": "extension.formatProject",
					"group": "4_Starling",
					"when": "editorTextFocus"
				}
			]
		},
		"keybindings": [
			{
				"command": "extension.formatProject",
				"key": "ctrl+f11",
				"mac": "cmd+f11",
				"when": "editorTextFocus"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"lint": "eslint . --ext .ts,.tsx",
		"watch": "tsc -watch -p ./"
	},
	"devDependencies": {
		"@commitlint/cli": "^11.0.0",
		"@commitlint/config-conventional": "^11.0.0",
		"@types/braces": "^3.0.0",
		"@types/debug": "^4.1.5",
		"@types/fs-extra": "^9.0.6",
		"@types/mkdirp": "^1.0.1",
		"@types/node": "^12.20.4",
		"@types/q": "^1.5.4",
		"@types/vscode": "^1.34.0",
		"@typescript-eslint/eslint-plugin": "^3.0.2",
		"@typescript-eslint/parser": "^3.0.2",
		"eslint": "^7.17.0",
		"fs-extra": "^9.1.0",
		"husky": "^4.3.7",
		"lint-staged": "^10.5.3",
		"prettier": "^2.2.1",
		"stylelint": "^13.8.0",
		"stylelint-config-standard": "^21.0.0",
		"stylelint-order": "^4.1.0",
		"stylelint-scss": "^3.19.0",
		"typescript": "^4.0.2"
	},
	"dependencies": {
		"braces": "^3.0.2",
		"debug": "^4.3.1",
		"fs": "0.0.1-security",
		"mkdirp": "^1.0.4",
		"path": "^0.12.7",
		"q": "^1.5.1"
	}
}
